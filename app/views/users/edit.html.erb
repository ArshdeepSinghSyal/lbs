<h1>Editing User</h1>

<% if current_user.id == @user.id || current_user.usertype == "admin" %>
  <%= render 'form', user: @user, libraries: @libraries %>
  <%= link_to 'Back', users_path %>
<% elsif user_signed_in? %>
  <p>You are not authorized to edit other users.</p>
  <%= link_to 'Back', users_path %>
<% else %>
  <%= link_to "You must be logged in to edit users.", new_user_session_path %>
<% end %>